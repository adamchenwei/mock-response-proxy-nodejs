{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 8081",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "5.2.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-hot-loader": "4.3.12",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.0.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "node-sass": "4.11.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.6.3",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.27.1",
    "webpack-dev-server": "3.1.10"
  }
}
